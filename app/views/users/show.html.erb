            <p> </p>
            </div>
            <div class="rowlg-8 d-flex justify-content-around margin m-5">
              <span class="badge bg-secondary">Editer Profil</span>
              <span class="badge bg-secondary">Supprimer</span>
              

            </div>
          
            
              

          </div>




          <div class="col-5 border border-secondary rounded mb-5">
            <h2>Vos commandes</h2>
            <div class="bs-component">
              <table class="table table-hover text-center">
                <thead>
                  <tr>
                    <th scope="col">Id</th>
                    <th scope="col">Date</th>
                    <th scope="col">Prix</th>
                    <th scope="col">Détail</th>
                  </tr>
                </thead>
                <tbody>
                  <tr class="table-secondary">
                    <th scope="row">Active</th>
                    <td>Column content</td>
                    <td>Column content</td>
                    <td>Column content</td>
                  </tr>
                  <tr class="table-light">
                    <th scope="row">Light</th>
                    <td>Column content</td>
                    <td>Column content</td>
                    <td>Column content</td>
                  </tr>
                </tbody>
              </table>
